{
    "name": "comodojo/extender-framework",
    "description": "Database driven, multiprocess, (pseudo) cron tasks scheduler",
    "license": "MIT",
    "keywords": [
        "comodojo",
        "extender",
        "framework",
        "scheduler",
        "cron",
        "multiprocess",
        "task",
        "cronjobs"
    ],
    "homepage": "https://extender.comodojo.org",
     "support": {
        "issues": "https://github.com/comodojo/extender.framework/issues"
    },
    "authors": [
        {
            "name": "Marco Giovinazzi",
            "email": "marco.giovinazzi@comodojo.org",
            "homepage": "http://jme.altervista.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Comodojo\\Extender\\": "src/Comodojo/Extender"
         }
    },
    "bin": [
        "bin/extender",
        "bin/extenderd"
    ],
    "require": {
        "php": ">=5.6",
        "comodojo/daemon": "^1.0",
        "comodojo/dispatcher.framework": "^4.0",
        "comodojo/cache": "^2.0",
        "mtdowling/cron-expression": "^1.0",
        "doctrine/orm": "^2.0",
        "league/fractal": "^0.17",
        "symfony/yaml": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "scrutinizer/ocular": "^1.0"
    },
    "replace": {
        "comodojo/extender.framework": "^1.0"
    }
}
