    "autoload": {
        "psr-4": {
            "Comodojo\\Exception\\": "src/Exception",
            "Comodojo\\Extender\\Job\\": "src/Job",
            "Comodojo\\Extender\\Scheduler\\": "src/Scheduler",
            "Comodojo\\Extender\\Shell\\Commands\\": "src/Shell/Commands",
