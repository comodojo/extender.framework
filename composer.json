{
    "name": "comodojo/extender.framework",
    "description": "Database driven, multiprocess, (pseudo) cron tasks scheduler in PHP",
    "license": "GPL-3.0+",
    "keywords": [
        "comodojo",
        "extender",
        "framework",
        "scheduler",
        "cron",
        "multiprocess"
    ],
    "homepage": "https://extender.comodojo.org",
     "support": {
        "issues": "https://github.com/comodojo/extender.framework/issues"
    },
    "authors": [
        {
            "name": "Marco Giovinazzi",
            "email": "marco.giovinazzi@comodojo.org",
            "homepage": "http://jme.altervista.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Comodojo\\Extender\\Command\\": "src/Command",
            "Comodojo\\Extender\\Job\\": "src/Job",
            "Comodojo\\Extender\\Runner\\": "src/Runner",
            "Comodojo\\Extender\\Scheduler\\": "src/Scheduler",
            "Comodojo\\Extender\\Shell\\": "src/Shell",
            "Comodojo\\Extender\\Task\\": "src/Task",
            "Comodojo\\Extender\\": "src"
         }
    },
    "require": {
        "php": ">=5.3.0",
        "pear/console_commandline": "dev-trunk",
        "pear/pear_exception": "*",
        "pear/console_table": "1.2.*",
        "pear/console_color2": "dev-master",
        "comodojo/database": "~1.0",
        "mtdowling/cron-expression": "1.0.*",
        "monolog/monolog": "~1.0",
        "comodojo/exceptions": "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "scrutinizer/ocular": "~1.1"
    }
}