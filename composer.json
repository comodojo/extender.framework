{
    "name": "comodojo/extender.framework",
    "description": "Comodojo extender framework",
    "license": "GPL-3.0+",
    "keywords": [
        "comodojo",
        "extender",
        "framework"
    ],
    "homepage": "http://extender.comodojo.org",
     "support": {
        "issues": "https://github.com/comodojo/extender.framework/issues"
    },
    "authors": [
        {
            "name": "Marco Giovinazzi",
            "email": "info@comodojo.org"
        }
